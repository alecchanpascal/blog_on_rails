<%= form_with model: @user do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control" %>
  </div>
  <br>
  <div class="form-group">
    <%= f.label :email %>
    <%= f.email_field :email, class: "form-control" %>
  </div>
  <br>
    <%= submit_tag :"Update User", class: "btn btn-outline-primary" %>
<% end %>
<br>
<%= link_to "Change Password", user_path(@user) %>