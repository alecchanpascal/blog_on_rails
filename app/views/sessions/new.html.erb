<h1>Sign In</h1>

<%= form_tag session_path do %>
  <div class="form-group">
    <%= label_tag :email %>
    <%= email_field_tag :email, "", class: "form-control" %>
  </div>
  <br>
  <div class="form-group">
    <%= label_tag :password %>
    <%= password_field_tag :password, "", class: "form-control" %>
  </div>
  <br>
  <div>
    <%= submit_tag "Log In", class: "btn btn-outline-primary" %>
  </div>
<% end %>